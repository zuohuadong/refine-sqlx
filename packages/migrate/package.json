{
  "name": "@refine-sqlx/migrate",
  "version": "0.1.0",
  "description": "Migration tools for refine-sqlx packages",
  "type": "module",
  "license": "MIT",
  "author": "RefineORM Team",
  "engines": {
    "node": ">=16.0.0"
  },
  "homepage": "https://github.com/zuohuadong/refine-sql#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zuohuadong/refine-sql.git",
    "directory": "packages/refine-migrate"
  },
  "bugs": {
    "url": "https://github.com/zuohuadong/refine-sql/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "refine",
    "migration",
    "database",
    "sql",
    "orm",
    "migration-tool",
    "typescript"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
