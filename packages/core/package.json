{
  "name": "@refine-sqlx/core",
  "version": "0.3.2",
  "private": true,
  "description": "Shared utilities for Refine data providers",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@refinedev/core": "^4.0.0"
  },
  "devDependencies": {
    "@refinedev/core": "5.0.4",
    "typescript": "^5.9.3",
    "unbuild": "3.6.1"
  },
  "keywords": [
    "refine",
    "data-provider",
    "utilities",
    "sql",
    "orm"
  ],
  "author": "RefineORM Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
